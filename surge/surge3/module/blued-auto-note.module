#!name=Blued - Auto note
#!desc=Blued automatically make note.

[MITM]
tcp-connection = true
hostname = %APPEND% argo.blued.cn, 106.75.55.121, 106.75.33.126, 106.75.70.21

[Script]
Blued = type=http-request,pattern=^https:\/\/argo\.blued\.cn\/users\/[0-9]+\/more\/.*$,script-path=https://raw.githubusercontent.com/ninjai/rules/master/surge/surge3/script/blued-get-id.js
Blued = type=http-request,pattern=^https:\/\/argo\.blued\.cn\/users\/[0-9]+\?.*$,script-path=https://raw.githubusercontent.com/ninjai/rules/master/surge/surge3/script/blued-get-headers.js
Blued = type=http-response,pattern=^https:\/\/argo\.blued\.cn\/users\/[0-9]+\?.*$,requires-body=true,script-path=https://raw.githubusercontent.com/ninjai/rules/master/surge/surge3/script/blued-auto-note.js

Blued = type=http-request,pattern=^https:\/\/[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+\/users\/[0-9]+\/more\/.*$,script-path=https://raw.githubusercontent.com/ninjai/rules/master/surge/surge3/script/blued-get-id.js
Blued = type=http-request,pattern=^https:\/\/[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+\/users\/[0-9]+\?.*$,script-path=https://raw.githubusercontent.com/ninjai/rules/master/surge/surge3/script/blued-get-headers.js
Blued = type=http-response,pattern=^https:\/\/[0-9]+\.[0-9]+\.[0-9]+\.[0-9]+\/users\/[0-9]+\?.*$,requires-body=true,script-path=https://raw.githubusercontent.com/ninjai/rules/master/surge/surge3/script/blued-auto-note.js