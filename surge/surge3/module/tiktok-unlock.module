#!name=TikTok
#!desc=Remove TikTok's regional restrictions and watermark.
#!system=ios

[URL Rewrite]
(?<=_region=)CN(?=&) US 307
(?<=&app_version=)17..(?=.?.?&) 1 307
(?<=\?version_code=)17..(?=.?.?&) 1 307

[MITM]
hostname = %APPEND% *.tiktokv.com, *.byteoversea.com, *.musical.ly, *.snssdk.com, *.amemv.com